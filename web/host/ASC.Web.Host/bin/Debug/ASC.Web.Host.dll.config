<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="webServer" type="ASC.Web.Host.Config.ServerSection, ASC.Web.Host"/>
  </configSections>

  <webServer
    scheme="http"
    port="8080"
    virtualPath="/"
    physicalPath="..\..\..\test\test_aspnet_site\"
    defaultDocument="Default.aspx"
    defaultFileNames="default.aspx,default.htm,default.html,index.htm,index.html"
    restrictedDirs="/bin,/_private_folder,/app_browsers,/app_code,/app_data,/app_localresources,/app_globalresources,/app_webreferences"
    bufferSize="65536"
    hackWCFBinding="true"
    authSchemes="Anonymous">

    <httpHandlers>
      <!-- extension="*" - default handler -->
      <!-- extension="/" - directory handler (.../url/) -->
      <add extension="*" handler="ASC.Web.Host.HttpHandlers.StaticFileHttpHandler" />
      <add extension="/" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="ad" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="adprototype" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="asa" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="asax" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="ascx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="ashx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="asmx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="asp" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="aspx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="axd" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="browser" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="cd" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="cdx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="cer" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="compiled" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="dll.config" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="exe.config" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="config" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="cs" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="csproj" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="dd" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="exclude" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="jdc" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="java" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="jsl" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="ldb" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="ldd" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="lddprototype" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="ldf" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="lic" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="licx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="master" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="mdb" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="mdf" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="msgx" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="refresh" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
