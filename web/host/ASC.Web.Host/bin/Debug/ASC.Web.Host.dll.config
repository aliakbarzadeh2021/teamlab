      <add extension="shtml" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="sitemap" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="skin" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="soap" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="stm" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
      <add extension="svc" handler="ASC.Web.Host.HttpHandlers.AspNetHttpHandler" />
